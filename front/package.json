    "build": "better-npm-run build",
    "start": "better-npm-run start-dev",
    "start-prod": "better-npm-run start-prod"
  },
  "betterScripts": {
    "build": {
      "command": "rimraf webpack/dist && webpack --config ./webpack/webpack.production.config.js --progress --profile --colors",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./app/server-index.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "development",
        "PORT": 3001
      }
    },
    "start-prod": {
      "command": "node ./app/server-index.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "production",
        "PORT": 9898
      }
    }
  },
